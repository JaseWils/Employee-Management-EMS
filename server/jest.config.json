{
  "testEnvironment": "node",
  "coveragePathIgnorePatterns": [
    "/node_modules/"
  ],
  "setupFilesAfterEnv": ["./tests/setup.js"],
  "testMatch": [
    "**/tests/**/*.test.js"
  ],
  "collectCoverageFrom": [
    "controllers/**/*.js",
    "models/**/*.js",
    "middleware/**/*.js",
    "!**/node_modules/**"
  ]
}